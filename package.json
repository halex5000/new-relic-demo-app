{
  "name": "new-relic-demo-app",
  "version": "0.1.0",
  "bin": {
    "new-relic-demo-app": "bin/new-relic-demo-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "xo --fix",
