  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/newrelic": "^7.0.4",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "aws-cdk": "2.47.0",
    "jest": "^27.5.1",
    "newrelic": "^9.5.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7",
    "xo": "^0.52.4",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.47.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.0.3",
    "launchdarkly-node-server-sdk": "^6.4.3",
    "source-map-support": "^0.5.21"
  },
  "xo": {
    "rules": {
      "import/extensions": "off",
      "no-new": "off",
      "unicorn/prefer-module": "off",
      "@typescript-eslint/no-unsafe-call": "off",
